{
  "version": 2,
  "builds": [
    { "src": "api/index.ts", "use": "@vercel/node" },
    { "src": "api/health.ts", "use": "@vercel/node" },
    { "src": "server/dist/client/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/health", "dest": "/api/health.ts" },
    { "src": "/api/(.*)", "dest": "/api/index.ts" },
    { "src": "/assets/(.*)", "dest": "/server/dist/client/assets/$1" },
    { "src": "/favicon.ico", "dest": "/server/dist/client/favicon.ico" },
    { "src": "/(.*)", "dest": "/server/dist/client/index.html" }
  ],
  "env": {
    "NODE_ENV": "production",
    "CORS_ORIGIN": "https://learninglab-ai.vercel.app"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["iad1"]
}
